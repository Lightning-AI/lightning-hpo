          },
          "framework": { "title": "Framework", "type": "string" },
          "cloud_compute": { "title": "Cloud Compute", "type": "string" },
          "num_nodes": { "title": "Num Nodes", "type": "integer", "default": 1 },
          "logger": { "title": "Logger", "type": "string" },
