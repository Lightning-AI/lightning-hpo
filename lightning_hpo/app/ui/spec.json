          "shared_folder": { "title": "Shared Folder", "type": "string" },
          "status": { "title": "Status", "type": "string", "default": "not_started" },
          "desired_state": { "title": "Desired State", "type": "string", "default": "running" },
          "url": { "title": "Url", "type": "string" }
