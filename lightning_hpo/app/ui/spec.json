        "type": "object",
        "properties": {
          "params": {
            "title": "Params",
            "type": "object",
            "additionalProperties": {
              "anyOf": [
                { "type": "number" },
                { "type": "integer" },
                { "type": "array", "items": { "type": "number" } },
                { "type": "array", "items": { "type": "string" } }
